{% extends 'base.html.twig' %}

{% block title %}Culture{% endblock %}

{% block body %}
    <h1>Culture</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ culture.id }}</td>
            </tr>
            <tr>
                <th>Flag</th>
                <td>{{ culture.flag }}</td>
            </tr>
            <tr>
                <th>Locale</th>
                <td>{{ culture.locale }}</td>
            </tr>
        </tbody>
    </table>

    <a id="backtl" href="{{ path('culture_index') }}">back to list</a>

    <button href="{{ path('culture_edit', {'id': culture.id}) }}" class="btn btn-warning">edit</button>

    {{ include('culture/_delete_form.html.twig') }}
{% endblock %}
